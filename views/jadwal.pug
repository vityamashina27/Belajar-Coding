extends layout
block content
    .container
        .row
            .col-md-12
                h1.center Jadwal Sholat
                table.table.center.table-bordered
                    tr
                        th(rowspan=2, style="vertical-align : middle;text-align:center;") Kota Cimahi
                        th Shubuh
                        th Dzuhur
                        th Ashar
                        th Maghrib
                        th Isya
                    tr
                        td  #{jadwals.Fajr}
                        td  #{jadwals.Dhuhr}
                        td  #{jadwals.Asr}
                        td  #{jadwals.Maghrib}
                        td  #{jadwals.Isha}
                        